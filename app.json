{
    "name": "The New York Stories",
    "displayName": "TheNewYorkStories",
    "expo": {
        "version": "1.0.0",
        "privacy": "public",
        "sdkVersion": "41.0.0",
        "orientation": "portrait",
        "name": "The New York Stories",
        "slug": "TheNewYorkStories",
        "entryPoint": "./src/index.js",
        "assetBundlePatterns": ["**/*"],
        "icon": "./src/assets/images/icon.png",
        "platforms": ["ios", "android", "web"],
        "splash": {
            "image": "./src/assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "packagerOpts": {
            "config": "metro.config.js",
            "sourceExts": ["js", "jsx"]
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "ios": {
            "supportsTablet": true
        }
    }
}
